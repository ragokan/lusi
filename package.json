{
  "name": "lusi-vscode",
  "displayName": "lusi",
  "description": "Soft but clear syntax theme",
  "version": "0.0.4",
  "publisher": "ragokan",
  "repository": {
    "type": "git",
    "url": "https://github.com/ragokan/lusi-vscode"
  },
  "scripts": {
    "start": "tsup"
  },
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Themes"
  ],
  "icon": "static/icon.png",
  "contributes": {
    "themes": [
      {
        "label": "lusi",
        "uiTheme": "vs-dark",
        "path": "./output/lusi.json"
      },
      {
        "label": "lusi-evening",
        "uiTheme": "vs-dark",
        "path": "./output/lusi-evening.json"
      },
      {
        "label": "lusi no-italics",
        "uiTheme": "vs-dark",
        "path": "./output/lusi-noItalics.json"
      },
      {
        "label": "lusi-evening no-italics",
        "uiTheme": "vs-dark",
        "path": "./output/lusi-evening-noItalics.json"
      }
    ]
  },
  "tags": [
    "soft",
    "clear",
    "pink",
    "dark theme",
    "color-theme",
    "monokai"
  ],
  "devDependencies": {
    "@swc/core": "^1.2.160",
    "@types/chroma-js": "^2.1.3",
    "@types/node": "^17.0.22",
    "chroma-js": "^2.4.2",
    "tsup": "^5.12.1"
  }
}
